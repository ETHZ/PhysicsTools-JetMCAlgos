# Flavour byReference
  module KT4byRef = JetPartonMatcher{
    InputTag jets              = kt4CaloJets 
    InputTag partons           = myPartons
    double coneSizeToAssociate = 0.3
  }

# Flavour byValue PhysDef
  module KT4byValPhys = JetFlavourIdentifier{
    InputTag srcByReference = KT4byRef
    bool physicsDefinition  = true
  }

# Flavour byValue AlgoDef
  module KT4byValAlgo = JetFlavourIdentifier{
    InputTag srcByReference = KT4byRef
    bool physicsDefinition  = false
  }

