# Flavour byReference
  module KT6byRef = JetPartonMatcher{
    InputTag jets              = kt6CaloJets 
    InputTag partons           = myPartons
    double coneSizeToAssociate = 0.3
  }

# Flavour byValue PhysDef
  module KT6byValPhys = JetFlavourIdentifier{
    InputTag srcByReference = KT6byRef
    bool physicsDefinition  = true
  }

# Flavour byValue AlgoDef
  module KT6byValAlgo = JetFlavourIdentifier{
    InputTag srcByReference = KT6byRef
    bool physicsDefinition  = false
  }

